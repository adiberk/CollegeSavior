
<div id="myCarousel" class="carousel slide" data-ride="carousel">
  <div class="carousel-caption">
      <p>College is tough,</p>
      <p>Let us give you a hand</p>
  </div>
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner carousel-imgs">
    <div class="item active">
      <img src="Carousel1.jpg" alt="Los Angeles">
    </div>

    <div class="item">
      <img src="images (1).jpg" alt="Chicago">
    </div>

    <div class="item">
      <img src="images.jpg" alt="New York">
    </div>
  </div>

  <!-- Left and right controls -->
  <a class="left carousel-control hide" href="#myCarousel" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control hide" href="#myCarousel" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right"></span>
    <span class="sr-only">Next</span>
  </a>
</div>

<div class = "container-fluid home-services">
  <div class = "col-lg-3 col-md-6 col-sm-6">
    <h3 class= "text-center">Moving</h3>
    <%=link_to moving_services_path do%>
    <img src="delivery.jpg" class="img-circle center-block" alt="Cinque Terre" width="260" height="236">
    <div class="overlay img-circle center-block">
      <div class="text">Need help moving into your new room, or out of your old one?</div>
    </div>
    <% end %>
  </div>
  <div class = "col-lg-3 col-md-6 col-sm-6">
    <h3 class= "text-center">Cleaning</h3>
    <img src="cleaning.jpg" class="img-circle center-block" alt="Cinque Terre" width="260" height="236">
    <div class="overlay img-circle center-block">
      <div class="text">Need your messy dorm room or apartment cleaned?</div>
    </div>
  </div>
  <div class = "col-lg-3 col-md-6 col-sm-6">
    <h3 class= "text-center">Tutoring</h3>
    <img src="tutoring.jpg" class="img-circle center-block" alt="Cinque Terre" width="260" height="236">
    <div class="overlay img-circle center-block">
      <div class="text">Need help with a writing assingmnet, or studying for a test?</div>
    </div>
  </div>
  <div class = "col-lg-3 col-md-6 col-sm-6">
    <h3 class= "text-center">Other</h3>
    <img src="all-services.jpg" class="img-circle center-block" alt="Cinque Terre" width="260" height="236">
    <div class="overlay img-circle center-block">
      <div class="text">Need help on something else? just ask!</div>
    </div>
  </div>
</div>